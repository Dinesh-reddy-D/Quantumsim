   "source": [
    "#Duetsch josza Algorithm for 2 qubit system with balanced function\n",
    "\n",
    "from qiskit import *\n",
    "%matplotlib inline\n",
    "import numpy as np\n",
    "from qiskit.visualization import plot_histogram"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "# setting the length of the String n\n"
    "n = 2\n",
    "\n",
    "oracle = \"b\"\n",
    "\n",
    "# if the function is balanced, set b\n",
    "if oracle == \"b\":\n",
    "    b = 3 \n",
    "\n",
    "# if the function is constant, set c = 0 or 1 randomly.\n",
    "if oracle == \"c\":\n",
    "    c = np.random.randint(2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 769.356x264.88 with 1 Axes>"
      ]
     },
     "execution_count": 3,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "qr = QuantumRegister(n+1)\n",
    "cr = ClassicalRegister(n)\n",
    "Cdj = QuantumCircuit(qr,cr)\n",
    "barriers = True\n",
    "Cdj.x(qr[n])\n",
    "if barriers:\n",
    "    Cdj.barrier()\n",
    "Cdj.h(qr)\n",
    "if barriers:\n",
    "    Cdj.barrier()\n",
    "if oracle == \"c\": # if the oracle is constant, return c\n",
    "    if c == 1:\n",
    "        Cdj.x(qr[n])\n",
    "    else:\n",
    "        Cdj.iden(qr[n])\n",
    "else:  \n",
    "    for i in range(n):\n",
    "        if (b & (1 << i)):\n",
    "            Cdj.cx(qr[i], qr[n])\n",
    "if barriers:\n",
    "    Cdj.barrier()    \n",
    "for i in range(n):\n",
    "    Cdj.h(qr[i])\n",
    "    \n",
    "for i in range(n):\n",
    "    Cdj.measure(qr[i],cr[i])\n",
    "backend= Aer.get_backend('qasm_simulator')\n",
    "result=execute(Cdj,backend=backend,shots=1024).result()\n",
    "answer=result.get_counts()\n",
    "Cdj.draw(output='mpl')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 504x360 with 1 Axes>"
      ]
     },
     "execution_count": 4,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "plot_histogram(answer)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "<AccountProvider for IBMQ(hub='ibm-q', group='open', project='main')>"
      ]
     },
     "execution_count": 5,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "from qiskit.providers.ibmq import least_busy\n",
    "IBMQ.load_account()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "least busy backend:  ibmqx2\n"
     ]
    }
   ],
   "source": [
    "provider= IBMQ.get_provider('ibm-q')\n",
    "provider.backends()\n",
    "backend = least_busy(provider.backends(filters=lambda x: x.configuration().n_qubits <= 5 and \n",
    "                                   not x.configuration().simulator and x.status().operational==True))\n",
    "print(\"least busy backend: \", backend)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Job Status: job has successfully run\n"
     ]
    }
   ],
   "source": [
    "from qiskit.tools.monitor import job_monitor\n",
    "shots = 1024\n",
    "job = execute(Cdj, backend=backend, shots=shots)\n",
    "job_monitor(job, interval = 2)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
